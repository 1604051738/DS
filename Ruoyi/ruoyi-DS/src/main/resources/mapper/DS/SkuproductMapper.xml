    <select id="selectSkuproductList" parameterType="Skuproduct" resultMap="SkuproductResult">
        <include refid="selectSkuproductVo"/>
        <where>  
            <if test="skucode != null  and skucode != ''"> and SKUCode like concat(#{skucode}, '%')</if>
            <if test="skuocode != null  and skuocode != ''"> and SKUOCode like concat('%', #{skuocode}, '%')</if>
