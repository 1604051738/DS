    <sql id="selectSkuproductVo">
        select id, SKUCode, SKUOCode, size, color, suttle, rw, costPrice, sellPrice, state, rank, ssq, ssqd, tsq, tsqd, tpq, pod, pd, stt, IPS, product, filePath, length, width, height, SKUMap from skuproduct
